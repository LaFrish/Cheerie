
<h2>Artists <%= link_to("(+)", new_artist_path) if @current_user %></h2>

<ul>
  <% @artists.each do |artist| %>
    <li>
      <%= link_to artist.name, artist %>
    </li>
  <% end %>
</ul>


<h2>Songs</h2>

<ul>
  <% @songs.each do |song| %>
    <li><%= link_to "#{song.title} - #{song.artist.name}", song %></li>
  <% end %>
</ul>
